{
  "coc.preferences.useQuickfixForLocations": true,
  "suggest.snippetIndicator": "üç¶",
  "diagnostic.showUnused": true,
  "diagnostic.showDeprecated": true,
  "languageserver": {
    "clojure-lsp": {
      "command": "clojure-lsp",
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "deps.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "diagnostic.showUnused": true,
      "diagnostic.showDeprecated": true,
      "diagnostic.highlightPriority": 1000000,
      "initializationOptions": {
        "ignore-classpath-directories": true
      },
    }
  },
  "clojure.enable": true,
  "clojure.lsp-version": "latest",
  "clojure.lsp-check-on-start": true,
  "clojure.keymaps.enable": true,
  "clojure.keymaps.shortcut": "<leader>r",
  "clojure.startup-message": true,
  "clojure.trace.server": "off",
} 
